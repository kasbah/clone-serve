{
  "name": "git-clone-and-serve",
  "version": "1.0.0",
  "description": "Git cloning and serving files as a service",
  "main": "lib/server.js",
  "scripts": {
    "prestart": "yarn run flow-remove",
    "start": "node lib/server.js",
    "flow-remove": "rm -f lib/*.js && flow-remove-types src/ --out-dir lib",
    "pretest": "yarn run flow-remove",
    "test": "flow && mocha test",
    "pretest:integration": "yarn run flow-remove",
    "test:integration": "mocha integration"
  },
  "pre-commit": [
    "test",
    "test:integration"
  ],
  "keywords": [],
  "author": "Kaspar Emanuel",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cookie-session": "^2.0.0-alpha.2",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "immutable": "^3.8.1",
    "is-git-url": "github:kasbah/is-git-url#ssh-user-and-not-dot-git",
    "list-filepaths": "^1.5.7",
    "redux": "^3.5.2",
    "shortid": "^2.2.6"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "flow-bin": "^0.36.0",
    "flow-remove-types": "^1.1.2",
    "flow-typed": "^2.0.0",
    "mocha": "^3.2.0",
    "pre-commit": "^1.1.3",
    "supertest": "^2.0.1"
  }
}
