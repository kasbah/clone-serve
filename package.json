{
  "name": "git-clone-and-serve",
  "version": "1.0.0",
  "description": "Git cloning and serving the files as a service",
  "main": "lib/server.js",
  "scripts": {
    "start": "node lib/server.js",
    "flow-remove": "rm -f lib/*.js && flow-remove-types src/ --out-dir lib",
    "pretest": "npm run flow-remove",
    "test": "flow && mocha test",
    "pretest:integration": "npm run flow-remove",
    "test:integration": "mocha integration --timeout=32000",
    "test:i" : "npm test:integration"
  },
  "engines": {
    "node": "6.9.2"
  },
  "keywords": [],
  "author": "Kaspar Emanuel",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cookie-session": "^2.0.0-alpha.2",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "immutable": "^3.8.1",
    "is-git-url": "github:kasbah/is-git-url#ssh-user-and-not-dot-git",
    "redux": "^3.5.2",
    "rimraf": "^2.5.4",
    "serve-index": "^1.8.0",
    "serve-static": "^1.11.1",
    "shortid": "^2.2.6"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "flow-bin": "^0.36.0",
    "flow-remove-types": "^1.1.2",
    "flow-typed": "^2.0.0",
    "mocha": "^3.2.0",
    "supertest": "^2.0.1"
  }
}
